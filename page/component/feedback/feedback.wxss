.line_none:before{
    border-top: 0;
}
.line_none:after{
    border-bottom: 0;
}
.card_radius {
  border-radius: 10px;
  border: 1px solid rgba(0,0,0,.05);
}
.checkbox{
  display: inline-block;
  margin: 5px;
  font-size: 26rpx;
  border: 1px solid #f04848;
  padding: 5px 10px;
  border-radius: 20px;
}
.checkboxselected{
  background: #f04848;
  color: #fff;
}
