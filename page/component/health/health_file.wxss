/* pages/my/health/health_file.wxss */

.line_none:before{
    border-top: 0;
}
.line_none:after{
    border-bottom: 0;
}
.card_radius {
  border-radius: 10px;
  border: 1px solid rgba(0,0,0,.05);
}

.card_acress:after {
  content: " ";
  display: inline-block;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #f04748;
  border-style: solid;
  margin-right: 4px;
}
.icon_a{
  width: 36rpx;
  height: 36rpx;
}
.dianming{
  display: inline-block;
  width: calc(100% - 85px);
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.scoll-h{
  height: calc(100% - 30rpx);
}
navigator{
  -webkit-tap-highlight-color: transparent;
  -moz-tap-highlight-color: transparent;
}
.weui-media-box__desc_1{
  color:#666;
  font-size:15px;
  line-height:2;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
}
.pinggu_jieguo{
  color: #ccc;
  line-height: 1.2;
  margin-top: 10rpx;
}
.card_left{
  width: 80px;
}
.card_right{
  width: calc(100% - 85px);
  margin-left: 5px;
}
.indexnone_img{
  width: 60%;
  position: fixed;
  top: 20%;
  left:20%;
  height: 400rpx;
  opacity: 0.5;
}
.indextextnone{
  font-size: 28rpx;
  color: #818f96;
  text-align: center;
  position: absolute;
  left: 0;
  top: 50%;
  width: 100%;
}
